{
  "name": "port",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@visx/responsive": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/text": "^3.3.0",
    "@visx/wordcloud": "^3.3.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "antd": "^5.24.5",
    "concurrently": "^8.2.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lottie-react": "^2.3.1",
    "object-hash": "^3.0.0",
    "pyodide": "^0.27.2",
    "react-highlight-words": "^0.20.0",
    "recharts": "^2.12.0",
    "typescript": "^4.4.3",
    "vite": "^6.0.11",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^2.1.4",
    "xhr-shim": "^0.1.3",
    "zod": "^3.22.4"
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "tsc -w",
    "clean": "rm -rf ./build",
    "build:css": "NODE_ENV=production tailwindcss build -i ./src/framework/styles.css -o ./build/styles.css --minify",
    "build:wheel": "cd src/framework/processing/py && poetry build --format wheel",
    "build:install-wheel": "cp -R src/framework/processing/py/dist/. ./public",
    "build:py": "npm run build:wheel && npm run build:install-wheel",
    "build:app": "tsx && vite build",
    "start:py": "nodemon --ext py --exec \"npm run build:py\"",
    "start:app": "vite",
    "start": "concurrently 'npm run start:py' 'npm run start:app'",
    "devmode": "npm run build:py && cross-env VITE_DEVMODE=true vite",
    "build": "npm run build:py && npm run build:app && npm run build:css",
    "release": "npm run build && ./release.sh $npm_package_name",
    "lint": "npm run fix:ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/lodash": "^4.14.185",
    "@types/node": "^22.13.1",
    "@types/object-hash": "^3.0.6",
    "@types/react-highlight-words": "^0.16.7",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "ts-standard": "^11.0.0"
  },
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "dist",
      "src/framework/processing/py_worker.js"
    ]
  }
}
